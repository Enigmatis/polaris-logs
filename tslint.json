{
    "defaultSeverity": "error",
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ],
    "jsRules": {},
    "extends": [
        "tslint:latest",
        "tslint-eslint-rules",
        "tslint-config-prettier"
    ],
    "rules": {
        "file-name-casing": [
            true,
            "kebab-case"
        ],
        "prettier": [
            true,
            ".prettierrc"
        ],
        "object-literal-sort-keys": false
    }
}